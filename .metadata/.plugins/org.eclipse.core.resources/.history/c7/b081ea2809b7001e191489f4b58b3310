package org.generation.collecttions;

import java.util.HashSet;

public class HashSetCollection {
	public static void main(String[] args) {
		//HashSet es una clase de Collections donde cada elemento es Ãºnice, es decir, no se repiten los elementos
		
		HashSet<String> animals = new HashSet<String>();
		
		animals.add("Buho");
		animals.add("Caballo");
		animals.add("Perro");
		animals.add("Gato");
		animals.add("Ornitorrinco");
		animals.add("Cebra");
		animals.add("Ornitorrinco");
		animals.add("Mapache");
		animals.add("Capibara");
		animals.add("Capibara");
		animals.add("Capibara");
		animals.add("Capibara");
		animals.add("Capibara");
		
		//Imprimir el HashSet
		System.out.println(animals);
		
	}//main
}//class
